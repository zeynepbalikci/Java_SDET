package Day4_arithmeticOperators;

public class arıtmetıcOperators {

	public static void main(String[] args) {
		
		System.out.println(55+5);
		
		int chairs = 140;
		System.out.println(chairs+2);
		System.out.println("chairs");
		System.out.println("chairs"+2);
		
		int morechairs = chairs+4;
		System.out.println(morechairs);
		
		int tables = 130;
		int total = tables+chairs;
		System.out.println(total);
		
		double balance = 1200.50;
		double transaction = 56.44;
		balance = balance-transaction;
		System.out.println(balance);
		
		int linesOfCode = 50;
		System.out.println("Line Of Code "+50);
		linesOfCode = linesOfCode - 2;
		System.out.println("Line Of Code "+linesOfCode);
		
		int classes = 22;
		System.out.println(linesOfCode * classes);
		
		//Multiplication
		System.out.println(22*2);
		
		//Division
		System.out.println(10/2);
		
			
		
		
		
	
				
			

	}

}
