package day2_printing_comments;

public class escapeSequences {

	public static void main(String[] args) {
		System.out.println("Learn\tJava\n\tthe\nEasy\tWay\n\n");
		System.out.println("\tLearn Java the \"Easy\" Way!\n");
		System.out.println("\\ // -=- \\ //");
		System.out.println("\\\\ \\\\\\ \\\\\\\\");

	}

}
